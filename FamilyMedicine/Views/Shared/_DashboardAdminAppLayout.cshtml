<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Family Medicine</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="shortcut icon" href="~/Content/imgs/logos/logoConColor.png" />

    <link rel="stylesheet" href="~/Content/css/DashboardCss/DashBoardGeneral.css" />
    <link href='https://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet'>

    <script src="https://kit.fontawesome.com/e77fc0ae51.js" crossorigin="anonymous"></script>
</head>
<body>
    
    <div class="container body-content">
        <section class="ContenedorDeDashboard">
            <section class="contenedorDeNav">
                <div class="mb-2 contenedorDeOpcionesDelNav A6 gap-1" style="width:100%">
                    <i class="fa-solid fa-arrow-left"></i>
                    <a> @Html.ActionLink("Inicio", "Index", "Home", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" }) </a>
                </div>
                <div class="mb-2 contenedorDeOpcionesDelNav A1 gap-1" style="width:100%">
                    <i class="fa-solid fa-flask-vial"></i>
                    <a> @Html.ActionLink("Laboratorios", "LaboratoriosRegistrados", "DashboardAdminApp", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" }) </a>
                </div>
                <div class="mb-2 contenedorDeOpcionesDelNav A2 gap-1" style="width:100%">
                    <i class="fa-solid fa-users"></i>
                    <a> @Html.ActionLink("Usuarios", "Usuarios", "DashboardAdminApp", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" }) </a>
                </div>
                <div class="mb-2 contenedorDeOpcionesDelNav A3 gap-1" style="width:100%">
                    <i class="fa-solid fa-calendar-days"></i>
                    <a> @Html.ActionLink("Actividades", "Actividades", "DashboardAdminApp", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" }) </a>
                </div>
                <div class="mb-2 contenedorDeOpcionesDelNav gap-1" style="width:100%">
                    <i class="fa-solid fa-right-from-bracket"></i>
                    <a> @Html.ActionLink("Cerrar Sesión", "Logout", "IniciarSecion", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" }) </a>
                </div>
            </section>

            <section class="contenedorDeHeader" style="padding-left: 170px">
                @RenderBody()
            </section>
        </section>
    </div>

    <script src="~/Scripts/Views/dashboardUsuario/DashboardValidacion.js"></script>
    <script src="https://kit.fontawesome.com/e77fc0ae51.js" crossorigin="anonymous"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
