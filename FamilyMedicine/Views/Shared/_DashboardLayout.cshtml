<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Family Medicine</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="shortcut icon" href="~/Content/imgs/logos/logoConColor.png" />

    <link rel="stylesheet" href="~/Content/css/DashboardCss/DashBoardGeneral.css" />
    <link href='https://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet'>

    <script src="https://kit.fontawesome.com/e77fc0ae51.js" crossorigin="anonymous"></script>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top">
        <div class="container">

            <a href="@Url.Action("Index", "Home")" title="Widget View" class="links">
                <img width="100px" alt="Widget View" src="@Url.Content("~/Content/imgs/logos/lgoBlanco.png")">
            </a>

            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarColor01">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item">
                        <a> @Html.ActionLink("Laboratorios", "Index", "Home", new { area = "" }, new { @class = "nav-link" })</a>
                    </li>
                    <li class="nav-item">
                        <a> @Html.ActionLink("Exámenes", "Index", "Home", new { area = "" }, new { @class = "nav-link" })</a>
                    </li>
                    <li class="nav-item">
                        <a> @Html.ActionLink("Quines somos", "LandingPage", "Home", new { area = "" }, new { @class = "nav-link" })</a>
                    </li>
                </ul>
                <div class="d-flex">
                    <ul class="navbar-nav me-auto">
                        <li class="nav-item">
                            <a> @Html.ActionLink("Iniciar Sesión", "Login", "Home", new { area = "" }, new { @class = "nav-link" }) </a>
                        </li>
                    </ul>
                    <ul class="navbar-nav me-auto">
                        <li class="nav-item">
                            <a> @Html.ActionLink("Carrito de Compras", "CarritoDeCompras", "Home", new { area = "" }, new { @class = "nav-link" }) </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    <div class="container body-content">
        <section class="ContenedorDeDashboard">
            <section class="contenedorDeNav">
                <a href="" class="mb-2 contenedorDeOpcionesDelNav A1">
                    <i class="fa-solid fa-user"></i>
                    <p class="ContenedorDeNombre">Usuario</p>
                </a>
                <a href="" class="mb-2 contenedorDeOpcionesDelNav A2">
                    <i class="fa-solid fa-flask-vial"></i>
                    <p class="ContenedorDeNombre">Mis Examenes</p>
                </a>
                <a href="" class="mb-2 contenedorDeOpcionesDelNav A3">
                    <i class="fa-solid fa-calendar-days"></i>
                    <p class="ContenedorDeNombre">Mis Citas</p>
                </a>
                <a href="" class="mb-2 contenedorDeOpcionesDelNav A4">
                    <i class="fa-solid fa-cart-shopping"></i>
                    <p class="ContenedorDeNombre">Mis Compras </p>
                </a>
                <div class="mb-2 contenedorDeOpcionesDelNav A5">
                    <i class="fa-solid fa-wrench"></i>
                    <a> @Html.ActionLink("Examenes", "DashAdminLabRegistrarExamen", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                </div>
                <div class="mb-2 contenedorDeOpcionesDelNav A6">
                    <i class="fa-solid fa-wrench"></i>
                    <a> @Html.ActionLink("Citas y Cupos", "DashAdminLabRegistrarCitas", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                </div>

                <div class="mb-2 contenedorDeOpcionesDelNav A7">
                    <i class="fa-solid fa-wrench"></i>
                    <a> @Html.ActionLink("Personal", "DashAdminLabRegistrarPersonal", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                </div>
                <div class="mb-2 contenedorDeOpcionesDelNav A8">
                    <i class="fa-solid fa-wrench"></i>
                    <a> @Html.ActionLink("Ventas", "DashAdminLabHistorialVentas", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                </div>
                <div class="mb-2 contenedorDeOpcionesDelNav A9">
                    <i class="fa-solid fa-wrench"></i>
                    <a> @Html.ActionLink("Laboratorio", "DashAdminLabDatosLaboratorio", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                </div>
            </section>

            <section class="contenedorDeHeader">
                @RenderBody()
            </section>
        </section>
    </div>


    <script src="https://kit.fontawesome.com/e77fc0ae51.js" crossorigin="anonymous"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
