<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Family Medicine</title>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/Views/ControlAcciones.js"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link rel="shortcut icon" href="~/Content/imgs/logos/logoConColor.png" />

    <link rel="stylesheet" href="~/Content/css/DashboardCss/DashBoardGeneral.css" />
    <link href='https://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet'>

    <script src="https://kit.fontawesome.com/e77fc0ae51.js" crossorigin="anonymous"></script>
</head>
<body>
  
    <div class="container body-content">
        <section class="ContenedorDeDashboard">
            <section class="contenedorDeNav">
                <div class="mb-2 contenedorDeOpcionesDelNav A6 gap-1" style="width:100%">
                    <i class="fa-solid fa-arrow-left"></i>
                    <a> @Html.ActionLink("Inicio", "Index", "Home", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" }) </a>
                </div>
                <div class="mb-2 contenedorDeOpcionesDelNav A1 gap-1" style="width:100%">
                    <i class="fa-solid fa-user"></i>
                    <a> @Html.ActionLink("Usuario", "DashboardUsuario", "DashboardUsuario", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" }) </a>
                </div>
                <div class="mb-2 contenedorDeOpcionesDelNav A2 gap-1" style="width:100%">
                    <i class="fa-solid fa-flask-vial"></i>
                    <a> @Html.ActionLink("Mis Exámenes", "DashBoardCitas", "DashboardUsuario", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" }) </a>
                </div>

                @if (Session["rolTecnico"] == null && Session["rolAdmin"] == null)
                {
                    <div class="mb-2 contenedorDeOpcionesDelNav A3 gap-1" style="width:100%">
                        <i class="fa-solid fa-flask-vial"></i>
                        <a> @Html.ActionLink("Registrar Laboratorio", "RegistrarLaboratorio", "DashboardUsuario", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                    </div>
                }

                @if (Session["rolTecnico"] != null || Session["rolAdmin"] != null)
                {
                    <div class="mb-2 contenedorDeOpcionesDelNav A5 gap-1" style="width:100%">
                        <i class="fa-solid fa-wrench"></i>
                        <a> @Html.ActionLink("Examenes", "DashAdminLabRegistrarExamen", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                    </div>
                }

                @if (Session["rolTecnico"] != null || Session["rolAdmin"] != null)
                {
                    <div class="mb-2 contenedorDeOpcionesDelNav A6 gap-1" style="width:100%">
                        <i class="fa-solid fa-wrench"></i>
                        <a> @Html.ActionLink("Citas y Cupos", "DashAdminLabRegistrarCitas", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                    </div>
                }

                @if (Session["rolTecnico"] != null || Session["rolAdmin"] != null)
                {
                    <div class="mb-2 contenedorDeOpcionesDelNav A10 gap-1" style="width:100%">
                        <i class="fa-solid fa-wrench "></i>
                        <a> @Html.ActionLink("Cupones", "Cupones", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                    </div>
                }

                @if (Session["rolAdmin"] != null)
                {
                    <div class="mb-2 contenedorDeOpcionesDelNav A7 gap-1" style="width:100%">
                        <i class="fa-solid fa-wrench"></i>
                        <a> @Html.ActionLink("Personal", "DashAdminLabRegistrarPersonal", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                    </div>
                }

                @if (Session["rolAdmin"] != null)
                {
                    <div class="mb-2 contenedorDeOpcionesDelNav A8 gap-1" style="width:100%">
                        <i class="fa-solid fa-wrench"></i>
                        <a> @Html.ActionLink("Ventas", "DashAdminLabHistorialVentas", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                    </div>
                }

                @if (Session["rolAdmin"] != null)
                {
                    <div class="mb-2 contenedorDeOpcionesDelNav A9 gap-1" style="width:100%">
                        <i class="fa-solid fa-wrench"></i>
                        <a> @Html.ActionLink("Laboratorio", "DashAdminLabDatosLaboratorio", "DashboardAdminLaboratorio", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" })</a>
                    </div>
                }

                <div class="mb-2 contenedorDeOpcionesDelNav gap-1" style="width:100%">
                    <i class="fa-solid fa-right-from-bracket"></i>
                    <a> @Html.ActionLink("Cerrar Sesión", "Logout", "IniciarSecion", new { area = "" }, new { @class = "ContenedorDeNombre text-decoration-none" }) </a>
                </div>


            </section>

            <section class="contenedorDeHeader" style="padding-left: 170px">
                @RenderBody()
            </section>
        </section>
    </div>

    <script src="~/Scripts/Views/dashboardUsuario/DashboardValidacion.js"></script>
    <script src="https://kit.fontawesome.com/e77fc0ae51.js" crossorigin="anonymous"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
